{
  "name": "@m0-foundation/ui-library",
  "version": "0.1.13",
  "description": "M0 Foundation UI Component Library based on Vue 3 and Tailwind CSS",
  "main": "dist/ui-library.umd.js",
  "module": "dist/ui-library.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/ui-library.es.js",
      "require": "./dist/ui-library.umd.js"
    },
    "./tailwind.preset.js": "./src/tailwind.preset.js"
  },
  "files": [
    "dist",
    "src/tailwind.preset.js",
    "src/components",
    "src/index.ts"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --declaration --emitDeclarationOnly && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "peerDependencies": {
    "tailwindcss": "^3.3.0",
    "vue": "^3.3.0",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "postcss": "^8.4.0",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.0.0",
    "vite": "^6.3.5",
    "vue-router": "^4.0.0",
    "vue-tsc": "^2.2.10"
  },
  "dependencies": {
    "vue-router": "^4.0.0"
  }
}
